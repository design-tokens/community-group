---
export interface Props {
  url: string;
}

const { url, ...props } = Astro.props;

type Icon = 'github' | 'opencollective' | 'linkedin' | 'x' | 'w3c' | 'bluesky';

let icon: Icon | undefined;

if (url.includes('bluesky.app') || url.includes('bsky.app')) {
  icon = 'bluesky';
} else if (url.includes('github.com')) {
  icon = 'github';
} else if (url.includes('linkedin.com')) {
  icon = 'linkedin';
} else if (url.includes('opencollective.com')) {
  icon = 'opencollective';
} else if (url.includes('w3.org')) {
  icon = 'w3c';
} else if (url.includes('x.com')) {
  icon = 'x';
}
---

